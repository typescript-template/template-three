{
	"name": "{{project-name}}",
	"version": "1.0.0",
	"typescript-template": {
		"base": "1.0.1",
		"jasmine": "1.0.0",
		"node": "1.0.1",
		"vite": "1.0.0"
	},
	"scripts": {
		"build": "tsc",
		"start": "ts-node src/index",
		"watch": "nodemon .",
		"watch-ui": "nodemon --config nodemon-ui.json",
		"test": "npm run lint && npm run build && nyc npm run test:run",
		"test:run": "jasmine --config=jasmine.json",
		"test:dev": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.dev.json",
		"doc": "typedoc --out docs/typedoc src",
		"format": "prettier-eslint --write **/*.*",
		"lint": "eslint .",
		"lint:prod": "npm run lint -- --config=.eslintrc.prod.json",
		"script": "ts-node scripts"
	},
	"dependencies": {
		"three": "^0.159.0",
		"ts-appconfig": "^1.2.0",
		"ts-async-bootstrap": "^2.1.0",
		"ts-error-handler": "^1.0.2",
		"ts-tiny-log": "^1.1.1"
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "^1.0.2",
		"@types/jasmine": "^5.1.4",
		"@types/node": "^18.19.39",
		"@types/three": "^0.159.0",
		"@typescript-eslint/eslint-plugin": "^7.13.1",
		"eslint": "^8.57.0",
		"jasmine": "^5.1.0",
		"nodemon": "^3.1.4",
		"nyc": "^17.0.0",
		"prettier": "^3.3.2",
		"prettier-eslint": "^16.3.0",
		"prettier-eslint-cli": "^8.0.1",
		"sass": "^1.77.6",
		"ts-node": "^10.9.2",
		"typedoc": "^0.26.1",
		"typescript": "~5.4.2",
		"vite": "^5.3.1"
	}
}
