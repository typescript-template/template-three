{
	"name": "{{project-name}}",
	"version": "1.0.0",
	"typescript-template": {
		"base": "1.0.1",
		"jasmine": "1.0.0",
		"node": "1.0.1",
		"vite": "1.0.0"
	},
	"scripts": {
		"build": "tsc",
		"start": "ts-node src/index",
		"watch": "nodemon .",
		"watch-ui": "nodemon --config nodemon-ui.json",
		"test": "npm run lint && npm run build && nyc npm run test:run",
		"test:run": "jasmine --config=jasmine.json",
		"test:dev": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.dev.json",
		"doc": "typedoc --out docs/typedoc src",
		"format": "prettier-eslint --write **/*.*",
		"lint": "eslint .",
		"lint:prod": "npm run lint -- --config=.eslintrc.prod.json",
		"script": "ts-node scripts"
	},
	"dependencies": {
		"three": "^0.159.0",
		"ts-appconfig": "^1.2.0",
		"ts-async-bootstrap": "^2.1.0",
		"ts-error-handler": "^1.0.2",
		"ts-tiny-log": "^1.1.1"
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "^1.0.2",
		"@types/jasmine": "^4.3.0",
		"@types/node": "^18.11.9",
		"@types/three": "^0.159.0",
		"@typescript-eslint/eslint-plugin": "^5.44.0",
		"@typescript-eslint/parser": "^5.44.0",
		"eslint": "^8.28.0",
		"jasmine": "^4.5.0",
		"nodemon": "^3.0.1",
		"nyc": "^15.1.0",
		"prettier": "^2.8.0",
		"prettier-eslint": "^15.0.1",
		"prettier-eslint-cli": "^7.1.0",
		"sass": "^1.69.5",
		"ts-node": "^10.9.1",
		"typedoc": "^0.23.21",
		"typescript": "^4.9.3",
		"vite": "^5.0.7"
	}
}
